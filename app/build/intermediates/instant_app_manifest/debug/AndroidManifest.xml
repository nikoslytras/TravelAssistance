            android:name="com.example.first_try.ENG_MAIN"
            android:label="Αγγλία"
            android:parentActivityName="com.example.first_try.MainActivity" />
        <activity android:name="com.example.first_try.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>