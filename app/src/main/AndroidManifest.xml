<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.first_try">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Coint"
            android:label="Μετατροπή Νομίσματος">

        </activity>
        <activity
            android:name=".FRA_MAIN"
            android:label="Γαλλία"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".IT_MAIN"
            android:label="Ιταλία"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Translate_ENG"
            android:label="Μεταφράσεις" />
        <activity
            android:name=".Translate_IT"
            android:label="Μεταφράσεις" />
        <activity
            android:name=".Translate_FRA"
            android:label="Μεταφράσεις" />
        <activity
            android:name=".ENG_MAIN"
            android:label="Αγγλία"
            android:parentActivityName=".MainActivity" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>